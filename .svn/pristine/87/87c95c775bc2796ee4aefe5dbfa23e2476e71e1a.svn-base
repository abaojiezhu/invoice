<mapper namespace="InvoiceMapper" >

	<select id="findById" parameterType="pd" resultType="pd">
		select
			*
		from invoice t
		where t.ID = #{invoiceId}
	</select>

	<select id="findInvoice" parameterType="pd" resultType="pd">
		select
			t.BILLING_NO,
			t.BILLING_CODE,
			t.CHECK_CODE,
			t.BILLING_TIME,
			t.FEE_WITHOUT_TAX,
			t.TAX,
			t.FEE
		from invoice t
		where t.USER_ID = #{userId}
		and t.BILLING_NO = #{billingNo}
		and t.BILLING_CODE = #{billingCode}
	</select>
	
	<select id="listByUser" parameterType="pd" resultType="pd">
		select
			t.ID,
    		t.BILLING_NO,
    		t.BILLING_CODE,
    		t.CHECK_CODE,
    		t.BILLING_TIME,
    		t.FEE_WITHOUT_TAX,
    		t.TAX,
    		t.FEE,
    		t.INVOICE_TYPE_NAME,
			t.SELLER_NAME,
			t.SELLER_NUMBER,
			t.SELLER_ADDRESS_AND_PHONE,
			t.SELLER_BANK_ACCOUNT,
			t.BUYER_NAME,
			t.BUYER_NUMBER,
			t.BUYER_ADDRESS_AND_PHONE,
			t.BUYER_BANK_ACCOUNT,
			t.CASHIER,
			t.MAKER,
			t.FUHR,
			t.REMARKS,
			t.VALID_STATE,
			t.INVOICE_SOURCE,
			t.FILE_NAME
		from invoice t
		where t.USER_ID = #{userId}
		<if test="invoiceSource != null">
			and t.INVOICE_SOURCE = #{invoiceSource}
		</if>
		<if test="billingTime != null">
			and t.BILLING_TIME = #{billingTime}
		</if>
		<if test="keyword != null">
			and t.SELLER_NAME like concat('%',#{keyword},'%')
		</if>
		<if test="sort != null">
			order by t.CREATED_DT #{sort}
		</if>
	</select>
	
